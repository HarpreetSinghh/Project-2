import pygame
import time
Display=pygame.display.set_mode((200,300),0,32)
GREEN=(0,225,0)
RED=(225,0,0)
class TrafficLight:
    pygame.draw.circle(Display,GREEN,(60,120),120,60)

traffic = TrafficLight()
traffic.TrafficLight
def green(self):
    pygame.draw.circle(Display,GREEN,(60,120),120,60)
def red(self):
    pygame.draw.circle(Display,RED,(60,120),120,60)

